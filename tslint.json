{
    "rules": {
      "import-spacing": true,
      "indent": [
        true,
        "spaces",
        4
      ],
      "max-line-length": [
        true,
        140
      ],
      "prefer-const": true,
      "quotemark": [
        true,
        "single"
      ],
      "semicolon": [
        true,
        "always"
      ],
      "triple-equals": [
        true,
        "allow-null-check"
      ]
    }
  }